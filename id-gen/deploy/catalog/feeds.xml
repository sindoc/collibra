<?xml version="1.0" encoding="UTF-8"?>
<!--
  feeds.xml — Atom feed registry for id-gen aggregator
  Each <feed href="…"/> is loaded by atom-aggregate.xsl via Saxon doc().
  Servers you control: guiti.be | khakbaz.com | markupware.com | lutinio.io
-->
<feeds xmlns:idgen="https://api-vlab.collibra.com/ontology/"
       idgen:catalog="deploy/catalog/catalog.xml">

  <!-- ── Your servers ─────────────────────────────────────────────────────── -->
  <feed href="https://markupware.com/atom.xml"
        idgen:theme="markupware"
        idgen:label="markupware.com"/>

  <feed href="https://khakbaz.com/atom.xml"
        idgen:theme="khakbaz"
        idgen:label="khakbaz.com"/>

  <feed href="https://guiti.be/atom.xml"
        idgen:theme="guiti"
        idgen:label="guiti.be"/>

  <feed href="https://lutinio.io/atom.xml"
        idgen:theme="lutinio"
        idgen:label="lutinio.io"/>

  <!-- ── Norman Walsh / xmlresolver news ──────────────────────────────────── -->
  <feed href="https://norm.walsh.name/atom.xml"
        idgen:label="norm.walsh.name"
        idgen:note="Norman Walsh — XML, XSLT, xmlresolver author"/>

  <!-- ── Collibra developer blog ──────────────────────────────────────────── -->
  <feed href="https://developer.collibra.com/feed.xml"
        idgen:label="Collibra Developer"/>

  <!-- ── W3C news ─────────────────────────────────────────────────────────── -->
  <feed href="https://www.w3.org/blog/feed/"
        idgen:label="W3C Blog"/>

  <!-- ── Local id-gen activity feed (generated by server) ─────────────────── -->
  <feed href="http://localhost:7331/api/feed"
        idgen:label="id-gen local"
        idgen:local="true"/>

</feeds>
